                                            </div>
                                            <button class="btn btn-success w-50" type="submit">@Localizer["Signin"]</button>
                                            <button class="btn btn-danger w-45" onclick="location.href='@Url.Action("Index", "Login")'">@Localizer["Register"]</button>

                                        </form>
                                    </div>
                                </li>
                            </ul>
                        </div><!-- .top-links end -->

                    </div>
                </div>

            </div>
        </div><!-- #top-bar end -->
        <!-- Header
        ============================================= -->
